<Window x:Class="TestSTF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestSTF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="Enter the ecuation!" HorizontalAlignment="Center" Margin="10" FontSize="25"/>
            <TextBox Name="stringField" Height="25" Width="200" Margin="20"/>
            <Button Name="btnCalculateResult" Content="CalculateResult" Height="25" Width="200" Click="btnCalculateResult_Click"/>
            <TextBlock Text="Result" HorizontalAlignment="Center" Margin="10" FontSize="25"/>
            <TextBox Name="resultField" Height="25" Width="200" Margin="20"/>
        </StackPanel>
    </Grid>
</Window>
